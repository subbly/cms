{
    "name":              "subbly/cms"
  , "version":           "0.1.0"
  , "type":              "project"
  , "keywords":          ["e-commerce", "cms"]
  , "license":           "MIT"
  , "minimum-stability": "stable"
  , "authors": [
        {
            "name":     "Benjamin Guedj"
          , "email":    "benjamin@subbly.com"
          , "role":     "Designer - founder"
        }
      , {
            "name":     "Michael Lefebvre"
          , "email":    "michael@subbly.com"
          , "role":     "Developer"
        }
      , {
            "name":     "Nicolas Brousse"
          , "email":    "nicolas@subbly.com"
          , "role":     "Developer"
        }
    ]
  , "config": {
        "vendor-dir":      "vendor"
      , "discard-changes": true
    }
  , "require": {
        "php":                         ">=5.4"
      , "ext-mcrypt":                  "*"
      , "laravel/framework":           "4.2.*"
      , "cartalyst/sentry":            "2.1.*"
      , "spatie/eloquent-sortable":    "0.*"
      , "league/oauth2-client":        "~0.3"
      , "barryvdh/laravel-debugbar":   "1.*"
      , "pimple/pimple":               "~3.0"
      , "symfony/property-access":     "~2.5"
      , "mustache/mustache":           "2.7.0"
    }
  , "require-dev": {
        "phpunit/phpunit":  "4.3.*"
      , "fzaninotto/faker": "1.*@dev"
    }
  , "autoload":
    {
      "classmap":
      [
          "app/commands"
        , "app/controllers"
        , "app/models"
        , "app/database/migrations"
        , "app/database/seeds"
        , "app/tests/TestCase.php"
      ]
    , "psr-0":
      {
        "": "lib"
      }
    }
  , "scripts":
    {
        "post-install-cmd":
        [
            "php artisan clear-compiled"
          , "php artisan optimize"
        ]
      , "post-update-cmd":
        [
            "php artisan clear-compiled"
          , "php artisan optimize"
        ]
      , "post-create-project-cmd":
        [
            "php artisan key:generate"
        ]
    }
}
